{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "jsdelivr",
      "library": "bootstrap@5.3.0",
      "files": [
        "dist/css/*.css",
        "dist/js/*.js"
      ],
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "library": "jquery@3.7.0",
      "destination": "wwwroot/lib/jquery/dist/",
      "files": [
        "jquery.js",
        "jquery.min.js",
        "jquery.min.map"
      ]
    }
  ]
}